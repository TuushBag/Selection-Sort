#include <iostream>



int main(int argc, char** argv) {
	
	int arr[] = {12,1,3,6,24,21,42,5,7,9};
	
	for( int i = 0; i < 10; i++){
		
		cout<< arr[i] << ",";
		
	}
	cout << endl;
	
	for( int i = 0, i < 10; i++){
		
		int smallestValue = arr[i];
		int smallestIndex = i;
		
		for( int x = i; x < 10; x++){
			if (arr[m] < smallest){
				smallestValue = arr[m];
				smallestIndex = m
			}
		}
		
		swap( arr[i], arr[smallestIndex]);
	}
	
	for( int i = 0; i < 10; i++){
		
		cout<< arr[i] << ",";
		
	}
	cout << endl;
	
	
	return 0;
}
